{
  "name": "developer.box.com",
  "description": "Box Developer Documentation - Configuration",
  "version": "1.0.0",
  "main": "index.yml",
  "repository": {
    "type": "git",
    "url": "https://github.com/box/developer.box.com.git"
  },
  "keywords": [
    "box",
    "platform",
    "api",
    "documentation"
  ],
  "author": "Box <oss@box.com>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "test": "yamllint ./src",
    "watch": "concurrently 'yarn:watch:*'",
    "watch:build": "watch 'yarn build' src compiler",
    "watch:test": "watch 'yarn test' src",
    "clean": "rm -rf compiled && mkdir compiled",
    "prebuild": "yarn clean",
    "build": "concurrently 'yarn:build:*'",
    "build:microcopy": "node -e 'require(\"./compiler\").write()'",
    "serve": "http-server -p 8081 ./compiled",
    "start": "concurrently 'yarn watch' 'yarn serve'"
  },
  "dependencies": {
    "@stoplight/json-ref-resolver": "2.0.1",
    "comment-json": "^2.0.0",
    "concurrently": "^4.1.0",
    "http-server": "^0.11.1",
    "js-yaml": "^3.13.1",
    "watch": "^1.0.2"
  }
}
