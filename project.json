{
    "name": "developer.box.com",
    "id": "developer",
    "description": "translate strings from the Box developer documentation",
    "projectType": "custom",
    "sourceLocale": "en-US",
    "resourceDirs": {
        "markdown": "locale"
    },
    "includes": [],
    "excludes": [
        ".git",
        "README.md",
	"node_modules",
	"compiler",
	"dictionaries",
	"markdownlintrc.json",
	"src/microcopy",
	"tests",
	"xliffs",
	"target"
    ],
    "settings": {
        "xliffsDir": "xliffs",
        "targetDir": "target",
        "locales": [
            "ja-JP"
        ]
    },
    "plugins": [
    	"ghfm"
    ]
}
